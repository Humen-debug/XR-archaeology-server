{
  "name": "xr-archaeology",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "npm run watch:css && next dev",
    "build": "npm run watch:css && next build",
    "start": "next start",
    "lint": "next lint",
    "watch:css": "postcss styles/tailwind.css -o styles/main.css",
    "devMongo": "mkdir -p data && mongod --dbpath data --replSet rs0",
    "docker:start": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "docker:force": "docker-compose up --build --force-recreate --renew-anon-volumes",
    "server:start": "ts-node -r tsconfig-paths/register server/index.ts"
  },
  "dependencies": {
    "@feathersjs/express": "^5.0.14",
    "@feathersjs/feathers": "^5.0.14",
    "@feathersjs/rest-client": "^5.0.14",
    "@feathersjs/socketio": "^5.0.14",
    "@feathersjs/socketio-client": "^5.0.14",
    "body-parser": "^1.20.2",
    "bson": "^6.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "feathers-mongoose": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.4",
    "next": "14.0.4",
    "react": "^18",
    "react-data-grid": "^7.0.0-beta.41",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "socket.io": "^4.7.3",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/webpack-env": "^1.18.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "postcss": "^8",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "webpack": "^5.89.0"
  }
}
